    "@typescript-eslint/parser": "^4.1.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "clipboard": "^2.0.6",
    "eslint": "^7.5.0",
